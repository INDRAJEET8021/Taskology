{
  "version": 2,
  "builds": [
    {
      "src": "backend/index.js",  
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/auth/google/callback",
      "destination": "/backend/auth/google/callback"  
    },
    {
      "source": "/(.*)",
      "destination": "/index.html" 
    }
  ]
}
